  <script>
    // needsclick klaviyo-form klaviyo-form-version-cid_1 kl-private-reset-css-Xuajs1

    function submitkl(elem){
      document.querySelector('.go338491780').click()
      // elem.style.display = 'none'

      setTimeout(() => {
        addpasscode()
      }, 300)
      
    }
    // lock functionality
    
    document.getElementById("locksmith_passcode_form").onsubmit = (e)=> {
      e.preventDefault()
      
      const code = document.getElementById('locksmith_passcode').value
      const valid = ['FBSKIIN', 'IGSKIIN', 'INSKIIN']
        if(valid.includes(code)){
          document.getElementById('customer-information-form').style.display = "flex" 
        }else{
          if(document.getElementById('locksmith_passcode_form-check').checked){
          addpasscode()
        } else{
          alert('Please read and accept the Contraindication to continue')
        }
        document.getElementById('customer-information-form').style.display = "none" 
      }
    };
    
  </script>


